{
  "configuration": {
	"appenders": {
	  "console": {
		"name": "Console-Appender",
		"target": "SYSTEM_ERR",
		"patternLayout": {
		  "pattern": "%d{DATE} [%level] - %msg%n"
		}
	  }
	},
	"loggers": {
	  "asyncRoot": {
		"level": "INFO",
		"appenderRef": {
		  "ref": "Console-Appender"
		}
	  }
	}
  }
}